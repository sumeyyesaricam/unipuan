@model UniPuan.Web.Models.UniModel
@{
    ViewBag.Title = "Sonuclar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="table table-striped table-bordered table-hover table-heading no-border-bottom">
    <thead>
        <tr>
            <th>Üniversite Adı</th>
            <th>Bölüm Adı</th>
            <th>Kontenjan</th>
            <th>Puan Türü</th>
            <th>Minimum Puan</th>
            <th>Sıralama</th>
        </tr>
    </thead>
    <tbody>
        
           @foreach (var typ in Model.Programs)
           {
            <tr>
                <td>@typ.UNIVERSITYNAME</td>
                <td>@typ.DEPARTMENTNAME</td>
                <td>@typ.QUOTAS</td>
                <td>@typ.SCORETYPE</td>
                <td>@typ.SCOREMIN</td>
                <td>@typ.ORDERR</td>               
            </tr>
           }
        
    </tbody>
</table>